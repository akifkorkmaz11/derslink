{
    "version": 2,
    "functions": {
      "server.js": { "runtime": "nodejs18.x", "maxDuration": 10 }
    },
    "routes": [
      {
        "src": "/css/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/js/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/images/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  